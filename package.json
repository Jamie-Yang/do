{
  "name": "tars-utils",
  "version": "1.0.1",
  "description": "JavaScript's functional programming helper library.",
  "main": "tars-utils.js",
  "module": "tars-utils.esm.js",
  "scripts": {
    "dev": "webpack-dev-server --progress --config ./build/webpack.config.js",
    "test": "rollup -w -c ./build/rollup.config.js",
    "build": "rollup -c ./build/rollup.config.js",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint --ext .js,.ts .",
    "eslint:fix": "eslint --ext .js,.ts --fix .",
    "prettier": "prettier -l \"./**/*.{js,json}\"",
    "prettier:fix": "prettier --write -l \"./**/*.{js,json}\"",
    "prepare-publish": "rm -rf dist_npm && mkdir dist_npm && cp package.json README.md ./dist_npm && npm run build"
  },
  "author": "Jamie Yang",
  "license": "ISC",
  "repository": "https://github.com/Jamie-Yang/tars-utils.git",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-alloy": "^3.6.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jsdoc": "^22.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "html-webpack-plugin": "^4.0.4",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
